/* pages/check_swiper/check_swiper.wxss */

.label {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 40rpx;
  font-size: 24rpx;
  background: rgba(0, 0, 0, 0.8);
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 10000;
}

.label .item {
  text-align: center;
  width: 50%;
  height: 40rpx;
  flex-shrink: 1;
  line-height: 40rpx;
}

.min {
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  z-index: 100;
  width: 40rpx;
  height: 40rpx;
  display: flex;
  justify-content: center;
  line-height: 36rpx;
  border-radius: 50%;
  background: rgba(0, 0, 0, 0.8);
  color: #fff;
  font-size: 20rpx;
}